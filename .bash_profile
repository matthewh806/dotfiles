
# Setting PATH for Python 2.7
# The orginal version is saved in .bash_profile.pysave
PATH="/Library/Frameworks/Python.framework/Versions/2.7/bin:${PATH}"
export PATH
export PATH=/Users/matthew/Development/android-sdk-macosx/platform-tools:/Users/matthew/Development/android-sdk-macosx/tools:$PATH
export PATH=/Library/Developer/Toolchains/swift-latest.xctoolchain/usr/bin:"${PATH}"
export PATH=~/bin:$PATH

alias xcrun-latest='xcrun launch-with-toolchain /Library/Developer/Toolchains/swift-latest.xctoolchain'
